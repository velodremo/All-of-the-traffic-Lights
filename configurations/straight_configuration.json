{
  "backend": "libsumo",
  "observation_radius": 100,
  "sumo_cmd": ["-c","/AI_project/nets/straight_net/config.sumocfg", "--random", "--no-warnings"],
  "net_dir": "/AI_project/nets/straight_net" ,
  "flow_probabilities": {"car": "0.003", "ambulance": "0.0003", "bus": "0.0003"},
